<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="db.mybatis.ElasticMapper.xml">
    <select id="selectNews" resultType="com.github.mm123mm123.Crawler.News">
        SELECT TITLE,CONTENT,LINK,CREATED_AT,MODIFIED_AT
        FROM NEWS
        LIMIT 2000
    </select>
</mapper>